{
  "env": {
      "node": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
    //"eslint:recommended"
   // "plugin:react/recommended",
   // "plugin:react/jsx-runtime"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    ],
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ]
  }
}